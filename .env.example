# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Set to "openai" to use OpenAI, "ollama" to use Ollama, or "mock" to use mock data
LLM_PROVIDER=openai

# Optional: Model to use with OpenAI (default: gpt-3.5-turbo)
OPENAI_MODEL=gpt-3.5-turbo

# Optional: Port for the server (default: 8000)
PORT=8000
